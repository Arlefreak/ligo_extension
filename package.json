{
  "name": "ligo.afk",
  "version": "1.0.0",
  "description": "ligo Browser Extension",
  "main": "js/script.js",
  "scripts": {
    "start:fox": "web-ext run -s dist --browser-console -p default",
    "test": "eslint js/* && web-ext lint -s dist",
    "build:chrome": "crx pack dist -o build/ligo.crx && web-ext build -s dist -a build",
    "build:fox": "web-ext build -s dist",
    "build:dev": "webpack --watch --progress",
    "build:prod": "webpack -p"
  },
  "repository": "git+https://github.com/Arlefreak/ligo_extension.git",
  "keywords": [
    "extension",
    "arlefreak",
    "afk",
    "ligo",
    "link"
  ],
  "author": "AFK",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Arlefreak/ligo_extension/issues"
  },
  "homepage": "https://github.com/Arlefreak/ligo_extension#readme",
  "devDependencies": {
    "archiver": "^1.3.0",
    "babel": "^6.23.0",
    "babel-loader": "^7.1.1",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "is": "^3.2.1",
    "nib": "^1.1.2",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^3.4.1",
    "archiver": "^1.3.0",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "crx": "^3.2.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0"
  }
}
