{
  "name": "ligo.afk",
  "version": "4.0.2",
  "description": "ligo Browser Extension",
  "main": "js/script.js",
  "scripts": {
    "start": "web-ext run",
    "start:dev": "webpack-dev-server --watch --progress --mode=development",
    "start:macos": "web-ext run -p default --firefox=/Applications/FirefoxDeveloperEdition.app/Contents/MacOS/firefox-bin",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "eslint --fix 'src/**/*.js'",
    "lint:format": "prettier --write 'src/**/*.js'",
    "test": "eslint js/* && web-ext lint -s dist",
    "build": "webpack --watch --progress --mode=development",
    "build:chrome": "crx pack dist -o build/ligo.crx && web-ext build -s dist -a build",
    "build:fox": "web-ext build -s dist --overwrite-dest",
    "build:prod": "webpack -p"
  },
  "repository": "git+https://github.com/Arlefreak/ligo_extension.git",
  "keywords": [
    "extension",
    "arlefreak",
    "afk",
    "ligo",
    "link"
  ],
  "author": "AFK",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Arlefreak/ligo_extension/issues"
  },
  "homepage": "https://github.com/Arlefreak/ligo_extension#readme",
  "lint-staged": {
    "src/**/*.{js,json,css}": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-syntax-export-default-from": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "crx": "^5.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^3.1.0",
    "is": "^3.3.0",
    "lint-staged": "^9.5.0",
    "prettier": "1.19.1",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "archiver": "^3.1.1",
    "autoprefixer": "^9.7.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "choo": "^7.0.0",
    "choo-devtools": "^3.0.3",
    "css-loader": "^3.4.1",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.9.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "style-loader": "^1.1.2",
    "web-ext": "^4.0.0",
    "webpack": "^4.41.5"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
