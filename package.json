{
  "name": "ligo.afk",
  "version": "3.0.0",
  "description": "ligo Browser Extension",
  "main": "js/script.js",
  "scripts": {
    "start": "web-ext run",
    "start:macos": "web-ext run -p default --firefox=/Applications/FirefoxDeveloperEdition.app/Contents/MacOS/firefox-bin",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "eslint --fix 'src/**/*.js'",
    "lint:format": "prettier --write 'src/**/*.js'",
    "test": "eslint js/* && web-ext lint -s dist",
    "build:chrome": "crx pack dist -o build/ligo.crx && web-ext build -s dist -a build",
    "build:fox": "web-ext build -s dist --overwrite-dest",
    "build:dev": "webpack --watch --progress --mode=development",
    "build:prod": "webpack -p",
    "precommit": "lint-staged"
  },
  "repository": "git+https://github.com/Arlefreak/ligo_extension.git",
  "keywords": [
    "extension",
    "arlefreak",
    "afk",
    "ligo",
    "link"
  ],
  "author": "AFK",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Arlefreak/ligo_extension/issues"
  },
  "homepage": "https://github.com/Arlefreak/ligo_extension#readme",
  "lint-staged": {
    "src/**/*.{js,json,css}": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "crx": "^5.0.1",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.5",
    "is": "^3.3.0",
    "lint-staged": "^9.2.5",
    "prettier": "1.18.2",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "archiver": "^3.1.1",
    "autoprefixer": "^9.6.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "choo": "^7.0.0",
    "choo-devtools": "^3.0.1",
    "css-loader": "^3.2.0",
    "dotenv": "^8.1.0",
    "eslint-config-prettier": "^6.1.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "style-loader": "^1.0.0",
    "web-ext": "^3.1.1",
    "webpack": "^4.39.3"
  },
  "peerDependencies": {}
}
