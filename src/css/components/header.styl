#header
  background $black
  color $white
  font-size 1.6em
  margin 0 0 10px 0
  padding 10px 0 10px 0
  .wrapper
    display flex
    flex-flow row wrap
    align-items center
    justify-content space-between
    div
      width 48%
      &:first-child
        text-align left
      &:last-child
          text-align right

